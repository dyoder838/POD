    <div class="row">
        <div class="col s12">
            <h2>Teacher</h2>
            <p class="flow-text">Pods associated with your classroom</p>
        </div>
    </div>
    <div class="row">
        <section class="col s12">
            <div class="row">
                {{#each teacher}}
                <div class="col s12 m12 l6">
                    <div class="card blue-grey darken-1">
                        <div class="card-content  white-text">
                            <h3>
                                <span class="thiqq">{{Parent.id}}</span>
                                 {{Parent.first_name}} {{Parent.last_name}}
                            </h3>
                            <ul>
                                <li>Monday: {{Parent.monday}}</li>
                                <li>Tuesday: {{Parent.tuesday}}</li>
                                <li>Wednesday: {{Parent.wednesday}}</li>
                                <li>Thursday: {{Parent.thursday}}</li>
                                <li>Friday: {{Parent.friday}}</li>
                            </ul>
                            {{#if Students}}
                            <h4>Students</h4>
                            <ul class="thinn">
                                {{#each Students}}
                                <li>{{first_name}} {{last_name}}</li>
                                {{/each}}
                            </ul>
                            {{/if}}
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </section>
    </div>
